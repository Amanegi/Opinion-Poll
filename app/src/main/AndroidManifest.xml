<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pravesh.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".common.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".common.LoginActivity" />
        <activity android:name=".admin.activities.AdminDashboardActivity" />
        <activity android:name=".admin.activities.AddAdminActivity" />
        <activity android:name=".admin.activities.AddUserActivity" />
        <activity android:name=".admin.activities.AddQuestionActivity"/>
        <activity android:name=".common.VoteActivity"/>
    </application>

</manifest>